{
  "name": "adaptive-math-quest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node server/leaderboard-server.mjs",
    "dev:full": "concurrently -k -n api,web -c cyan,magenta \"npm:dev:api\" \"npm:dev -- --host\"",
    "build": "tsc -b && vite build",
    "test": "vitest run",
    "verify:flow": "tsx scripts/verify-flow.ts",
    "verify:puzzles": "tsx scripts/verify-puzzles.ts",
    "verify:ui": "tsx scripts/verify-ui.ts",
    "verify:gen": "tsx scripts/verify-gen.ts",
    "verify:gen:ci": "tsx scripts/verify-gen.ts --ci",
    "preview": "vite preview"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "express": "^5.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.1",
    "jsdom": "^24.1.1",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
    "vitest": "^2.0.4"
  }
}
